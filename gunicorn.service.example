[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=dydeo
Group=www-data
WorkingDirectory=/home/dydeo/Silk_Road
ExecStart=/home/dydeo/Silk_Road/venv/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind unix:/run/gunicorn.sock \
          config.wsgi:application

[Install]
WantedBy=multi-user.target

